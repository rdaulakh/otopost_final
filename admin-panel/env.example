# AI Social Media Admin Panel - Environment Configuration

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# Authentication
VITE_JWT_STORAGE_KEY=admin_token
VITE_REFRESH_TOKEN_KEY=admin_refresh_token

# Application Settings
VITE_APP_NAME=AI Social Media Admin Panel
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_MULTI_TENANT=true
VITE_ENABLE_AI_AGENTS=true
VITE_ENABLE_SYSTEM_MONITORING=true

# Real-time Updates
VITE_UPDATE_INTERVAL=30000
VITE_WEBSOCKET_URL=ws://localhost:3000/ws

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp,video/mp4,video/webm

# Security
VITE_ENABLE_CSRF_PROTECTION=true
VITE_SESSION_TIMEOUT=3600000

# Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true

# Development
VITE_DEBUG_MODE=false
VITE_MOCK_API=false

